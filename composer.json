{
    "name": "sammyjo20/wagonwheel",
    "description": "Offer an online version of your Laravel emails to users.",
    "version": "2.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sammyjo20",
            "email": "sam.carre2000@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "illuminate/support": "^8.0",
        "illuminate/mail": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/console": "^8.0",
        "paquettg/php-html-parser": "^3.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sammyjo20\\Wagonwheel\\WagonwheelServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Sammyjo20\\Wagonwheel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Database\\Factories\\": "database/factories",
            "Sammyjo20\\Wagonwheel\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    }
}
